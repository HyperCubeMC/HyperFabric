  "version": "${version}",

  "name": "HyperFabric",
  "description": "HyperFabric is a mod developed for my Minecraft server HyperCubeMC to add essential features such as custom commands.",
  "authors": [
    "Justsnoopy30"
  ],
  "contact": {
    "email": "justsnoopy30@hypercubemc.cf",
