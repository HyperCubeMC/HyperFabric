    "homepage": "https://hypercubemc.cf",
    "issues": "https://github.com/hypercubemc-github/HyperFabric/issues",
    "sources": "https://github.com/hypercubemc-github/HyperFabric"
  },

  "license": "AGPL-3.0",
  "icon": "assets/hyperfabric/icon.png",

  "environment": "server",
  "entrypoints": {
    "main": [
      "net.hypercubemc.hyperfabric.Mod"
    ]
